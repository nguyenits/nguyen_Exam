   <header>
        <h1>Manager Question</h1>
    </header>
  <div class="editques-container">
<table>
  <thead>
    <tr class="editques">
      <th>ID</th>
      <th>Question</th>
      <th>Anser 1</th>
      <th>Anser 2</th>
      <th>Anser 3</th>
      <th>Anser 4</th>
      <th>Right anser</th>
      <th>Role</th>
      <th>level</th>
      <th colspan="3">action</th>
    </tr>
  </thead>
  <tbody>
    <% @question.each do |question| %>
      <tr>
        <td><%= question.id %></td>
        <td><%= question.quetion %></td>
        <td><%= question.a1 %></td>
        <td><%= question.a2 %></td>
        <td><%= question.a3 %></td>
        <td><%= question.a4 %></td>
        <td><%= question.ans %></td>
        <td><%= question.role%></td>
        <td><%= question.level%></td>
        <td><%= link_to 'Show', question %></td>
        <td><%= link_to 'Edit', edit_question_path(question) %></td>
        <td><%= link_to 'destroy', question, method: :delete, data: {confirm: 'Are you sure?'}%></td>
      </tr>
    <% end %>
  </tbody>
</table>
  </div>
<%== pagy_nav(@pagy) %>
<%= link_to 'New Question', new_question_path %>
<footer>
  <p>View my project <a href="#">NguyenExam&trade;</a></p>
</footer>