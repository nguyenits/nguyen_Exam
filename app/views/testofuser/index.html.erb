    <header>
        <h1>Score</h1>
    </header>
  <tbody>
<div style="padding: 2em 4em;">
<table>
   <%if current_user.role== "admin" %>
   <tr class="tes_user">

   <th>username</th>
   <th>email</th>
    <th>score</th> 
     <th>created_at</th>
    <th></th>
   </tr>
       <% @testofuserforadmin.each do |testofuser| %>
      <tr class="tes_user">
      <%user = User.find(testofuser['userID'].to_i)%>
        <td><%=user.username%> </td>
        <td><%=user.email%> </td>
        <td><%= testofuser['score'].to_f.round(2) %></td>
        <td><%=testofuser['created_at']%>
        <td><%= link_to 'Show', testofuser_path(testofuser['id'])%></td>
      </tr>
    <% end %>
   <%else%>
 <tr class="tes_user">
   <th>created_at</th>
   <th>score</th>
    <th></th>
   </tr>
    <% @testofuser.each do |testofuser| %>
      <tr class="tes_user">
         <td><%= testofuser.created_at%>
        <td><%= testofuser.score.to_f.round(2) %></td>
     
        <td><%= link_to 'Show', testofuser %></td>
      </tr>
    <% end %>       
    <%end%>
  </tbody>
</table>
</div>
    <footer>
        <p>View my project <a href="#">NguyenExam&trade;</a></p>
    </footer>